[user]
    name = "Zack Douglas"
    email = zdouglas@t-sciences.com
[color]
    ui = auto
[core]
    editor = emacs
[alias]
    a = add
    br = branch
    cfg = !${EDITOR:-emacs -nw} ~/.gitconfig
    ci = commit
    cim = commit -m
    co = checkout
    di = diff
    get-extras = !cd /tmp && git clone --depth 1 https://github.com/visionmedia/git-extras.git && cd git-extras && sudo make install
    lg = !git --no-pager log --abbrev-commit --decorate --oneline --graph --all $*
    lpr = !git ls-remote $1 -h *$2* -
    u = !git remote -f update --prune $1 && echo && git st
    st = status -s -uno --porcelain
    stt = status
